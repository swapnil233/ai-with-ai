    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.54.0"
  }
}
